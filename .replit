entrypoint = "main.py"
modules = ["nodejs-20", "python-3.11"]

[deployment]
run = ["python3", "main.py"]
deploymentTarget = "cloudrun"

[[ports]]
localPort = 3000
externalPort = 3000

[[ports]]
localPort = 3001
externalPort = 3001

[[ports]]
localPort = 8000
externalPort = 80

[[ports]]
localPort = 8080
externalPort = 8080

[nix]
packages = ["libxcrypt", "libyaml", "nano", "ngrok-1", "wget"]
